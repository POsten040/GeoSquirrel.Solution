@{
  Layout = "_Layout";
}

@using System.Security.Claims

<hr />
@if (User.Identity.IsAuthenticated)
{
    <p>Hello @User.Identity.Name, welcome back!</p>
    @using (Html.BeginForm("LogOff", "Account"))
    {
        <input type="submit" class="btn btn-dark" value="Log out" id="logout"/>
        <br>

    }
    @Html.ActionLink("View Cache List", "Index", "Cache") 
}
else
{
    <p>@Html.ActionLink("Register", "Register")</p>
    <p>@Html.ActionLink("Log in", "Login")</p>
}